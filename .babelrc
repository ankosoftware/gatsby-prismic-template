{
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    ["prismjs", {
      "languages": ["javascript", "css", "markup", "html", "clike", "shell"],
      "plugins": ["line-numbers"],
      "theme": "twilight",
      "css": true
    }]
  ],
  "presets": [
    [
      "babel-preset-gatsby",
      {
        "targets": {
          "browsers": [">0.25%", "not dead"]
        }
      }
    ]
  ]
}
